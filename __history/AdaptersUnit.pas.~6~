unit AdaptersUnit;

interface

uses
  System.Generics.Collections;

type
  Adapters = interface
    function getMenu: TList<string>;
    procedure setTest(caption: string);
    function getQuest: TList<string>;
    function getAnswerTrue: TList<string>;
    function getAnswerFalse: TList<string>;
  end;

implementation

end.
